{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": ["error", {
      "allow": ["warn", "info", "error", "debug"]
    }],
    "no-debugger": "error",
    "no-alert": "error"
  },
  "overrides": [
    {
      "files": ["lib/utils/logging/*.ts"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}