    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "antd": "^5.20.6",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.454.0",
    "moment": "^2.30.1",
    "nanoid": "^5.1.5",
    "next": "^14.2.32",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
